<!DOCTYPE html>
<html lang="de">
<head>
  <%- include('../partials/header') %>
  <title>Creators verwalten</title>
</head>
<body>
  <main>
    <h1>Creator-Konten</h1>
    <ul>
      <% creators.forEach(creator => { %>
        <li>
          <%= creator.username %> – <%= creator.email %>
          <form action="/admin/creators/delete/<%= creator._id %>" method="POST" style="display:inline;">
            <button type="submit">Löschen</button>
          </form>
        </li>
      <% }) %>
    </ul>
  </main>
</body>
</html>
